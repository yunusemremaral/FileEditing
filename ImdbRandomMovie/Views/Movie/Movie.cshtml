@model List<MovieRatingModel>

<h2>Top 10 Movies</h2>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Votes</th>
            <th>Rating</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>@movie.Title</td>
                <td>@movie.Votes</td>
                <td>@movie.Rating</td>
            </tr>
        }
    </tbody>
</table>
